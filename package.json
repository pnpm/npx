{
  "name": "@zkochan/libnpx",
  "version": "13.0.0",
  "description": "support library for npx -- an tool for executing npm-based packages.",
  "main": "index.js",
  "man": "./libnpx.1",
  "files": [
    "*.js",
    "libnpx.1",
    "locales"
  ],
  "scripts": {
    "bin": "make bin",
    "publish-bin": "npm run bin && cd bin && npm publish",
    "docs": "tail -n +2 README.md | marked-man --manual 'User Commands' --version \"$npm_package_name@$npm_package_version\" > $npm_package_name.1",
    "prerelease": "npm t && npm run docs",
    "postrelease": "npm publish && git push --follow-tags",
    "pretest": "standard *.js test bin/*.js locales",
    "release": "standard-version -s",
    "test": "cross-env NPX_UPDATE_LOCALE_FILES=true LC_ALL=en nyc --all -- tap -J test/*.js",
    "update-coc": "weallbehave -o . && git add CODE_OF_CONDUCT.md && git commit -m 'docs(coc): updated CODE_OF_CONDUCT.md'",
    "update-contrib": "weallcontribute -o . && git add CONTRIBUTING.md && git commit -m 'docs(contributing): updated CONTRIBUTING.md'"
  },
  "repository": "https://github.com/zkochan/npx",
  "keywords": [
    "npm",
    "npm exec",
    "shell",
    "scripts",
    "npm bin",
    "cli"
  ],
  "author": {
    "name": "Kat MarchÃ¡n",
    "email": "kzm@sykosomatic.org",
    "twitter": "maybekatz"
  },
  "license": "ISC",
  "dependencies": {
    "@zkochan/npm-package-arg": "^2.0.0",
    "dotenv": "^8.2.0",
    "enquirer": "^2.3.6",
    "rimraf": "^3.0.0",
    "safe-buffer": "^5.2.0",
    "which": "^2.0.2",
    "y18n": "^4.0.0",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "json": "^9.0.6",
    "marked-man": "^0.7.0",
    "mkdirp": "^0.5.1",
    "npm": "^6.13.4",
    "nyc": "^14.1.1",
    "require-inject": "^1.4.4",
    "standard": "^14.3.1",
    "standard-version": "^7.0.1",
    "tacks": "^1.3.0",
    "tap": "^14.10.2",
    "weallbehave": "^1.2.0",
    "weallcontribute": "^1.0.9"
  },
  "config": {
    "nyc": {
      "exclude": [
        "node_modules/**",
        "test/**"
      ]
    }
  },
  "engines": {
    "node": ">=10.13"
  }
}
